function Main()
  SN = gg.choice({
    "1.攻击速度",
    "2.攻击速度复原",
    "退出脚本"
  }, nil, "KOF97")
  if SN == 1 then
    a()
  end
  if SN == 2 then
    b()
  end
  
  if SN == 3 then
    Exit()
  end
  XGCK = -1
end

function a()
gg.clearResults()

gg.searchNumber("110;1036831949;1036831949;981668463;105;-1;0;100::100", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.searchNumber("105", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
local gui=gg.getResults(1)
gg.clearResults()
gg.searchAddress(string.format("%X",gui[1].address+72),-1,4)
gg.getResults(1)
t = gg.getResults(1)
t[1].value = '2'
t[1].freeze = true
print('addListItems: ', gg.addListItems(t))
local gui=gg.getResults(1)
gg.clearResults()
gg.searchAddress(string.format("%X",gui[1].address+4),-1,4)

gg.getResults(2)
t = gg.getResults(2)
t[1].value = '200'
t[1].freeze = true
print('addListItems: ', gg.addListItems(t))
gg.toast("变速成功")
end

function b()
gg.clearResults()
gg.searchNumber("110;1036831949;1036831949;981668463;105;-1;2;200::100", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
